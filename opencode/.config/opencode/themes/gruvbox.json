    "bright_orange": "#fe8019",
    "orange": "#d65d0e",
    "bright_purple": "#d3869b",
    "purple": "#b16286",
    "red": "#cc241d",
    "bright_red": "#fb4934",
    "bright_yellow": "#fabd2f",
    "yellow": "#d79921",
